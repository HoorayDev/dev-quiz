{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/share-library/(.*)": "<rootDir>/../libs/share-library/$1",
    "@app/share-library": "<rootDir>/../libs/share-library/"
  }
}